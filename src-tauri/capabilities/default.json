{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "fs:default",
    {
      "identifier": "fs:allow-app-write-recursive",
      "allow": [
        {
          "path": "$APPLOCALDATA/*"
        }
      ]
    }
  ]
}

